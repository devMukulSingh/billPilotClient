{
  "name": "billManagement",
  "version": "1.0.0",
  "packageManager": "bun@1.1.42",
  "scripts": {
    "build": "turbo run build",
    "lint": "turbo lint",
    "dev": "dotenv -- turbo run dev",
    "format": "cd apps/client && bunx prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "cd apps/client && bunx tsc --noEmit",
    "prepare": "husky"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "dependencies": {
    "@vercel/remix": "^2.15.3",
    "dotenv-cli": "^8.0.0",
    "lucide-react": "0.469.0",
    "react-icons": "^5.5.0"
  },
  "devDependencies": {
    "husky": "^9.1.7",
    "turbo": "^2.4.0"
  },
  "engines": {
    "node": ">=20"
  }
}
